{
  "name": "solana-arb-bot",
  "version": "1.0.0",
  "description": "高性能 Solana DEX 套利机器人 - 经济模型核心",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "tsx packages/core/src/index.ts",
    "build": "tsc -b packages/core packages/onchain-bot",
    "clean": "rimraf packages/*/dist",
    "test": "jest --config=./jest.config.js --no-coverage",
    "test:watch": "jest --config=./jest.config.js --watch",
    "test:coverage": "jest --config=./jest.config.js --coverage",
    "test:unit": "jest --config=./jest.config.js tests/unit",
    "test:integration": "jest --config=./jest.config.js tests/integration",
    "test:performance": "jest --config=./jest.config.js tests/performance --testTimeout=60000",
    "test:benchmark": "jest --config=./jest.config.js tests/performance/benchmark.test.ts",
    "test:stress": "jest --config=./jest.config.js tests/performance/stress.test.ts --testTimeout=60000",
    "test:all": "jest --config=./jest.config.js",
    "cost-sim": "tsx tools/cost-simulator/index.ts",
    "jito-monitor": "tsx tools/jito-monitor/index.ts",
    "demo": "tsx examples/economics-demo.ts",
    "test-jupiter": "tsx examples/test-jupiter-swap.ts",
    "start:onchain-bot": "tsx packages/onchain-bot/src/index.ts",
    "start:onchain-bot:prod": "node packages/onchain-bot/dist/index.js"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.14"
  }
}
