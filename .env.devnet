# ========================================
# Solana Arbitrage Bot - Devnet Configuration
# ========================================
# 这是 Devnet 测试环境配置
# 用于在测试网上验证系统功能

# ========================================
# Solana RPC 端点 (Devnet)
# ========================================
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_RPC_URLS=https://api.devnet.solana.com

# ========================================
# 密钥配置
# ========================================
# 使用 devnet 测试钱包
DEFAULT_KEYPAIR_PATH=./keypairs/devnet-test-wallet.json

# ========================================
# Jito 配置 (Devnet)
# ========================================
JITO_BLOCK_ENGINE_URL=https://amsterdam.devnet.block-engine.jito.wtf

# ========================================
# Jupiter 配置
# ========================================
# Jupiter 在 devnet 可能不可用，使用模拟模式
JUPITER_API_URL=http://127.0.0.1:8080
JUPITER_PUBLIC_API_URL=https://quote-api.jup.ag/v6

# ========================================
# 性能配置
# ========================================
RPC_POOL_SIZE=3
RPC_MAX_CONCURRENT=5
LOG_LEVEL=debug

# ========================================
# 安全配置 (Devnet - 宽松限制)
# ========================================
# Devnet 上可以使用更大的测试金额
MAX_TRANSACTION_AMOUNT=1000000000
MIN_WALLET_BALANCE=0.1

# ========================================
# 开发配置
# ========================================
NODE_ENV=development
DRY_RUN=false
VERBOSE=true

# ========================================
# Worker Threads
# ========================================
WORKER_THREADS=2

# ========================================
# 缓存配置
# ========================================
CACHE_TTL=30
METRICS_INTERVAL=30

# ========================================
# 代理配置（如果需要）
# ========================================
HTTP_PROXY=
HTTPS_PROXY=
NO_PROXY=localhost,127.0.0.1
